#define _CRT_SECURE_NO_WARNINGS

#include <iostream>
#include <stdio.h>
#include <conio.h>

void BubbleSort(char* str)
{
	int i = 0; int j = 0;
	int len = strlen(str);
	int swapped = 1;

	while(swapped)
	{
		swapped = 0;
		j++;
		for(i = 0; i < len - j; i++)
		{
			if(str[i] > 64 && str[i] < 91)
			{
				str[i] += 32;
			}
			if(str[i+1] > 64 && str[i+1] < 91)
			{
				str[i+1] += 32;
			}

			if(str[i+1]<str[i])
			{
				char temp = str[i];
				str[i] = str[i+1];
				str[i+1] = temp;
				swapped = 1;
			}
		}
	}
}

int ifUnique(char* str)
{
	BubbleSort(str);
	int i = 0;
	int len = strlen(str);
	while(i < len)
	{
		if(str[i+1] == str[i])
		{
			return 0;
		}
		i++;
	}

	return 1;
}
int main()
{

	//FILE *file = fopen("input.txt", "r");
	char line[1024];
	gets(line);
//	while(fgets(line, 1024, file))
	{
		if(ifUnique(line))
			printf("String is unique\n");
		else
			printf("String contains duplicate characters\n");
	}

	_getch();
	return 0;
}
